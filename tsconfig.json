{
  "$schema": "http://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "esnext",
		"moduleResolution": "node",
    "allowJs": false,
    "strictNullChecks": false,
		"noEmit": true,
		"strict": true,
		"isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react"
	},
	"include": [
		"./src"
  ],
  "exclude": [
    "**/*node_modules/**/*"
  ]
}